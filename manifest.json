{
  "manifest_version": 2,
  "name": "Toolbar speedDial",
  "version": "1.0.0",
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab"
  ],
  "description": "Toolbar Dial browser.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },

  "icons": { "16": "images/icon16.png",
    "48": "images/icon48.png",
   "128": "images/icon128.png"
  }

}
